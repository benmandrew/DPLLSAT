-include Makefile.user

OPAM = opam
DEPEXT ?= $(OPAM) pin add -k path --no-action --yes mirage-unikernel-main-macosx . &&\
	$(OPAM) depext --yes --update mirage-unikernel-main-macosx ;\
	$(OPAM) pin remove --no-action mirage-unikernel-main-macosx

.PHONY: all depend depends clean build
all:: build

depend depends::
	$(DEPEXT)
	$(OPAM) install -y --deps-only .

build::
	mirage build

clean::
	mirage clean

